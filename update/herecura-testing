#!/bin/bash
source $(dirname $0)/../config

cd ${rpdir}

for rpfolder in 'testing-any' 'testing'
do
	folders=$(ls -1 ${rpdir}/${rpfolder})
	for folder in ${folders}
	do
		if [ -d ${rpdir}/${rpfolder}/${folder} ];
		then
			buildone '' ${rpfolder}/${folder}
		fi
	done
done

cd ${rpdir}
git push origin master
git reset --hard
