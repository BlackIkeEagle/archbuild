# initialize default path locations
initPath=$(cd $(dirname $BASH_SOURCE) && pwd)
startPath=$(cd "$initPath/../" > /dev/null 2>&1 && pwd)
binPath=$(cd "$startPath/bin" > /dev/null 2>&1 && pwd)
etcPath=$(cd "$startPath/etc" > /dev/null 2>&1 && pwd)
sharePath=$(cd "$startPath/share" > /dev/null 2>&1 && pwd)
source "$sharePath/functions"

# vim:set ft=sh:
